{
    "name" : "fw3/strings",
    "description" : "Flywheel3 framework: string processing",
    "type" : "library",
    "homepage" : "https://ickx.jp",
    "license" : "MIT",
    "authors" : [{
            "name" : "akira wakaba",
            "homepage" : "https://ickx.jp",
            "role" : "lead developer"
        }
    ],
    "config" : {
        "autoloader-suffix" : "IckxFw3Strings"
    },
    "autoload" : {
        "psr-4" : {
            "fw3\\strings\\" : "./src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "fw3\\tests\\strings\\" : "./tests/"
        }
    },
    "keywords" : [
        "fw3 Flywheel3 string processing"
    ],
    "require" : {
        "php" : ">=7.2.0",
        "ext-mbstring" : "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~8.0.0"
    }
}
